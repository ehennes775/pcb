name=gettext
version=0.18.3
release=1
license=gpl3
origin=gnu
configure_opt="
  --enable-relocatable
"

# Solve an incompatibility with newer MinGW runtimes
ac_cv_func_getopt_long_only=no
export ac_cv_func_getopt_long_only

ac_cv_have_decl__snprintf__snwprintf=yes
export ac_cv_have_decl__snprintf__snwprintf
